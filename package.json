  "repository": {
    "type": "git",
    "url": "git+https://github.com/anovandri/kipkip-ui-toolkit.git"
  },
  "keywords": [
    "ui-toolkit",
    "kipkip"
  ],
  "author": "kipkip",
  "license": "MIT",
  "bugs": {
